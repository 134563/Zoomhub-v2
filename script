	Shidashian.MouseButton1Down:connect(function()
		local unanchoredparts = {}
		local movers = {}
		local tog = true
		local move = false
		local Player = game:GetService("Players").LocalPlayer
		local Character = Player.Character
		local tor = Character.Torso
		local mov = {};
		local mov2 = {};

