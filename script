		if TheGunHandle.Parent == Effects then	
			combothingy:disconnect()
			TheGunHandle:Destroy()
			attack = false
		end
		while true do Swait()

			if attack == false then 
				break
			end	

		end	
		print("aaaaaaaaaaaaaaaaaa")
